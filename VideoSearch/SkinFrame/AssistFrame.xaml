<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VideoSearch.SkinFrame"
             xmlns:SkinControl="clr-namespace:VideoSearch.SkinControl" x:Class="VideoSearch.SkinFrame.AssistFrame"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <SkinControl:SkinFrame x:Name="mainFrame" CornerRadius="0" HasTitlebar="False" CanResize="False" BkColor="{StaticResource WindowBkColor}"/>
    </Grid>
</UserControl>
